const express = require('express');

const app = express();
const PORT = 8080;

app.set('view engine', 'ejs');

app.use('/', function(req, res, next){
  res.render('home')
  next();
});

app.get('/', function(req, res){
  console.log("Working...")
  res.send();
});

app.listen(PORT, function(err){
  if (err) console.log(err);
  console.log("Website listening on port: ", PORT);
})
